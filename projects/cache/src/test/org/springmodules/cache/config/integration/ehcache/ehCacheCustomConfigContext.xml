<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:cache="http://www.springmodules.org/schema/cache"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop.xsd
       http://www.springmodules.org/schema/cache http://www.springmodules.org/schema/cache/springmodules-cache.xsd">

  <cache:config id="customCacheProvider" provider="EHCACHE"
    configLocation="classpath:org/springmodules/cache/config/integration/ehcache/ehcache-config.xml"
    serializableFactory="XSTREAM" failQuietly="true"/>

</beans>
